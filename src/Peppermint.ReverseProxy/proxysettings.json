{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "defaultCluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [
          { "PathPrefix": "/" },
          { "RequestHeadersCopy": "true" },
          { "RequestHeaderOriginalHost": "true" }
        ]
      }
    },
    "Clusters": {
      "defaultCluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://json-schema.org/"
          }
        }
      }
    }
  }
}